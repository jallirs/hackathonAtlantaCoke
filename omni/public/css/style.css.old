.button {
	border-radius: 3px;
	font-weight: 200;
	color: #fff;
	border: 1px solid #818181;
	border-color: #818181; 
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#acacac), to(#909090));
	background-image: -webkit-linear-gradient(top, #, #909090); 
	background-image: -moz-linear-gradient(top, #acacac, #909090);
	background-image: -ms-linear-gradient(top, #acacac, #909090);
	background-image: -o-linear-gradient(top, #acacac, #909090);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#acacac', endColorstr='#909090');
	background-color: #909090;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

.button.primary {
	border: 1px solid #3fab03;
	border-color: #3fab03;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#73d81b), to(#47be11));
	background-image: -webkit-linear-gradient(top, #73d81b, #47be11); 
	background-image: -moz-linear-gradient(top, #73d81b, #47be11);
	background-image: -ms-linear-gradient(top, #73d81b, #47be11);
	background-image: -o-linear-gradient(top, #73d81b, #47be11);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#73d81b', endColorstr='#47be11');
	background-color: #47be11;
}

.button.active {
	border: 1px solid #46b3d0;
	border-color: #46b3d0;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#46b3d0), to(#359ac1));
	background-image: -webkit-linear-gradient(top, #46b3d0, #359ac1); 
	background-image: -moz-linear-gradient(top, #46b3d0, #359ac1);
	background-image: -ms-linear-gradient(top, #46b3d0, #359ac1);
	background-image: -o-linear-gradient(top, #46b3d0, #359ac1);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#46b3d0', endColorstr='#359ac1');
	background-color: #359ac1;
}

.button.negative {
	border: 1px solid #f36114;
	border-color: #d8490f;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f36114), to(#d8490f));
	background-image: -webkit-linear-gradient(top, #f36114, #d8490f); 
	background-image: -moz-linear-gradient(top, #f36114, #d8490f);
	background-image: -ms-linear-gradient(top, #f36114, #d8490f);
	background-image: -o-linear-gradient(top, #f36114, #d8490f);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#f36114', endColorstr='#d8490f');
	background-color: #d8490f;
}


.button.disabled:focus,
.button.active.disabled:focus,
.button.negative.disabled:focus,
.button.primary.disabled:focus, 
.button.disabled:hover,
.button.active.disabled:hover,
.button.negative.disabled:hover,
.button.primary.disabled:hover, 
.button.disabled,
.button.active.disabled,
.button.negative.disabled,
.button.primary.disabled {
	cursor: default;
	background-color: #e9e9e9;
	border: 1px solid #e9e9e9;
	border-top: 1px solid #e9e9e9;
	color: #555;
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#e9e9e9), to(#e9e9e9));
	background-image: -webkit-linear-gradient(top, #, #e9e9e9); 
	background-image: -moz-linear-gradient(top, #e9e9e9, #e9e9e9);
	background-image: -ms-linear-gradient(top, #e9e9e9, #e9e9e9);
	background-image: -o-linear-gradient(top, #e9e9e9, #e9e9e9);
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr='#e9e9e9', endColorstr='#e9e9e9');
	-webkit-box-shadow:none;
}


