.fileUpload {
    border: 3px dashed #ccc;
    border-radius: 3px; 
    -webkit-user-select: none;
    cursor: pointer;
    outline: none;      
    width: 100%;         
    vertical-align: middle;
    text-align: center;
    display: inline-block; 
    color: #aaa;    
    font-size: 1.2em;
    vertical-align:middle;
	padding: 45px 0px;
	margin-bottom: 1em;
}

.fileUpload:hover,
.fileUpload.hovering {
    border: 3px dashed #40adde;
	color: #40adde;
}

.fileUpload input {
    position: absolute;
    top: 0;
    right: 0; 
    cursor: pointer;
    opacity: 0.0;
    filter: alpha(opacity=0); 
   /* font-size: 300px;         /* only reliable way to make a large part of the input clickable in most browsers */
	width: 100%;
    height: 100px;
}



.toggle {
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	-webkit-user-select: none;
	margin:4px;
	margin-left: 0;
	display: inline-block;;
}

.toggle label {
	float:left;
	width:auto;
	min-width: 70px;
	margin:0px;
	padding-top: 0;
	padding-bottom: 0px !important;
	color: #777;
	background-color:#FFF;
	border:1px solid #BBB;
	border-left:0px solid #DDD;
	overflow:auto;
	cursor: pointer;
	white-space:nowrap;
}


.toggle label:first-child {
	border-left:1px solid #BBB;
	border-radius: 4px 0px 0px 4px;
	white-space:nowrap;
}


.toggle label:last-child {
	border-radius: 0px 4px 4px 0px;
	border-right:1px solid #BBB;
	white-space:nowrap;
}


.toggle label span {
	text-align:center;
	padding: 7px;
	white-space:nowrap;
	display:block;
}


.toggle label input {
	position:relative;
	overflow: hidden;
	white-space:nowrap;
	display: none;
}

.toggle input:checked + span {
	white-space:nowrap;
	color:#40adde;
	background-color:#FFF;
}

.specialtoggle{
	margin-top: 0.5em;
	margin-left: 1em;
	padding-top: 0px;
	padding-bottom: 0px;
	color: #7c7c7c;
}

.specialtoggle input:checked + span {
	background-color: #40adde !important;
	color:#FFF !important;
}



.downArrow {
	content:'';
	display:inline-block;
	width:0;
	height:0;
	border-color: #999 transparent transparent transparent;
	border-style: solid;
	border-bottom: none;
	border-width: 4px;
	top: -3px;
	position: relative;
}

.upArrow {
	content:'';
	display:inline-block;
	width:0;
	height:0;
	border-color: transparent transparent #999 transparent;
	border-style: solid;
	border-top: none;
	border-width: 4px;
	top: -3px;
	position: relative;
}



